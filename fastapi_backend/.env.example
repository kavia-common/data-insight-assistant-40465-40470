# Application
APP_NAME=Data Insight Assistant
APP_ENV=development
LOG_LEVEL=INFO
PORT=3001
CORS_ALLOWED_ORIGINS=*

# MongoDB
# Example: mongodb://localhost:27017 or mongodb+srv://<user>:<pass>@<cluster>/<db>?retryWrites=true&w=majority
MONGO_URI=
MONGO_DB_NAME=
MONGO_COLLECTION=
MONGO_PING_ON_STARTUP=true

# Feature Flags
ENABLE_SUPABASE=false
ENABLE_NLQ=true
ENABLE_NLQ_AI=false

# Supabase (required only if ENABLE_SUPABASE=true)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# AI Providers (optional / future)
OPENAI_API_KEY=
