# FastAPI backend environment example

# Application metadata
APP_NAME="Data Insight Assistant"
APP_ENV="development"
LOG_LEVEL="INFO"

# Networking
PORT=3001
CORS_ALLOWED_ORIGINS="*"

# Database (REQUIRED for DB-backed endpoints to function)
# Use a valid Postgres connection string, e.g.:
# postgresql://user:password@host:5432/postgres
SUPABASE_DB_CONNECTION_STRING=""

# Feature flags
ENABLE_SUPABASE=false
ENABLE_NLQ=true
ENABLE_NLQ_AI=false

# Supabase (required only when ENABLE_SUPABASE=true)
SUPABASE_URL=""
SUPABASE_ANON_KEY=""

# Optional third-party keys
OPENAI_API_KEY=""
